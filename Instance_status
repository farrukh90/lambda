def lambda_handler(event, context):
    ec2 = boto3.resource('ec2')
    #ec2.create_instances(ImageId='ami-09693313102a30b2c', MinCount=1, MaxCount=5)
    for status in ec2.meta.client.describe_instance_status()['InstanceStatuses']:
